{
  "name": "select-and-run-without-debug",
  "displayName": "Select and Run Without Debug",
  "description": "Select and Run Without Debug from command palette.",
  "version": "1.1.2",
  "publisher": "quatters",
  "license": "SEE LICENSE IN LICENSE",
  "repository": {
    "url": "https://github.com/Quatters/select-and-run-without-debug"
  },
  "engines": {
    "vscode": "^1.72.0"
  },
  "categories": [
    "Keymaps",
    "Other"
  ],
  "keywords": [
    "debug",
    "debugging",
    "command",
    "palette"
  ],
  "icon": "images/icon.png",
  "activationEvents": [
    "onCommand:select-and-run-without-debug.activate"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "select-and-run-without-debug.activate",
        "title": "Debug: Select and Run Without Debug"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.0.0",
    "@types/mocha": "^10.0.0",
    "@types/node": "16.x",
    "@types/vscode": "^1.72.0",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "@vscode/test-electron": "^2.1.5",
    "eslint": "^8.52.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "glob": "^8.0.3",
    "mocha": "^10.0.0",
    "prettier": "3.0.3",
    "typescript": "^4.9.5"
  }
}
